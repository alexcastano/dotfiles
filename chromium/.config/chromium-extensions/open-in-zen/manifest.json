{
  "manifest_version": 3,
  "name": "Open in Zen",
  "version": "1.0",
  "description": "Opens external links in Zen Browser with correct container",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
